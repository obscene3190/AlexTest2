#include <iostream>
#include <utility>
#include <sstream>

using namespace std;

void perestanovka(double* &array, int size) {
    for(int i = 0; i < size/2; i++) {
        swap(array[i],array[size-i-1]);
    }
}

void sdvig(double* &array, int size, int a) {
    double *array1 = new double [size-a];
    double *array2 = new double [a];
    for(int i = 0; i < size-a; i++) {
        array1[i]=array[i];
    }
    for(int i = 0; i < a; i++) {
        array2[i]=array[a + i];
    }
    perestanovka(array1, size-a);
    perestanovka(array2, a);
    for(int i = 0; i < size-a; i++) {
        array[i]=array1[i];
    }
    for(int i = 0; i < a; i++) {
        array[a + i] = array2[i];
    }
    perestanovka(array, size);
}

bool read (istream & stream, double* &array, int size) {
    bool res = true;
    for (int i=0; i < size; i++) {
        if (!(stream >> array[i])) {
            res = false;
            break;
        }
    }
    if (!(stream.eof())) {
        cout << "Error";
        exit(0);
    }
    return res;
}

void write(ostream & stream,double* array, int size) {
    for (int i = 0; i < size; i++) {
        stream << array[i] << ' ';
    }
}

int main() {
    int size, a;
    double* array;
    if(!(cin >> size) || size < 1) {
        cout << "An error has occured while reading numbers from line";
        return -1;
    }
    cin.get();
    string string;
    getline(cin, string);
    istringstream stream(string);
    array = new double [size];
    if( read(stream,array, size)) {
        if(!(cin >> a) || a < 1) {
            cout << "An error has occured while reading numbers from line";
            return -1;
        }
        cin.get();
        sdvig(array, size, a);
        write (cout, array, size);
    }
    else {
        cout << "An error has occured while reading numbers from line";
    }
    delete[] array;
}
